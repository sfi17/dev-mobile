{% extends 'base.html.twig' %}

{% block title %}Parking{% endblock %}

{% block body %}
    <h1>Parking</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ parking.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ parking.title }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ parking.price }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ parking.rating }}</td>
            </tr>
            <tr>
                <th>Spots</th>
                <td>{{ parking.spots }}</td>
            </tr>
            <tr>
                <th>Free</th>
                <td>{{ parking.free }}</td>
            </tr>
            <tr>
                <th>Coordinate</th>
                <td>{{ parking.coordinate }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ parking.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('parking_index') }}">back to list</a>

    <a href="{{ path('parking_edit', {'id': parking.id}) }}">edit</a>

    {{ include('parking/_delete_form.html.twig') }}
